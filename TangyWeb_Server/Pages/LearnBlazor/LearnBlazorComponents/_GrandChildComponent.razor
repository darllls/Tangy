<div class="row">
    <h4 class="text-primary p-lg-4 pt-2 col-12">Grand Child Component</h4>
    <p>@MessageForGrandChild</p>
    <p>@LuckyNumberFromGrandParent</p>
</div>

@code {
    [CascadingParameter(Name ="message")]
    public string MessageForGrandChild { get; set; }
    [CascadingParameter(Name ="number")]
    public string LuckyNumberFromGrandParent{ get; set; }
}
